      "plugin_slug": "soyuz-blocks-templates-default",
      "type": "tpl",
      "category": "buttons",
      "blocks": [
        {
          "blockName": "core/button",
          "attrs": {
            "className": "",
            "actions": [
              {
                "set": {
                  "source": "insert_block_target_slug",
                  "value": "{this.source_slug}"
                }
              },
              {
                "get_collection": {
                  "source": "pages",
                  "query_variables": {
                    "slug": "{this.output.0}"
                  }
                }
              },
              {
                "set": {
                  "source": "insert_block",
                  "value": "{this.output.1.0.blocks.0.innerBlocks}"
                }
              },
              {
                "set": {
                  "source": "insert_block.0.attrs.foo",
                  "value": "bar"
                }
              }
            ]
          },
          "innerBlocks": [
            {
              "blockName": "core/button",
              "attrs": {
                "className": "-left -pad-xs -hvr-invert",
                "content": "Button 1"
              }
            }
          ]
        }
      ]
