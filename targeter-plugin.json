        "plugin_slug": "soyuz-targeter-app",
        "className": "-btn-bg -left -b -b-light-gray -pad-s -hvr-invert",
        "content": "Open Targeter",
        "actions": [
          {
            "set": {
              "source": "targeter",
              "value": true
            }
          },
          {
            "set": {
              "source": "editable",
              "value": true
            }
          },
          {
            "routerQuery": {
